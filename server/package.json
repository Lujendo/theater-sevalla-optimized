{
  "name": "theater-equipment-catalog-backend",
  "version": "1.0.0",
  "description": "Backend for Theater Equipment Catalog Management System",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "NODE_ENV=development nodemon index.js",
    "dev:setup": "node ../scripts/dev-setup.js",
    "dev:reset": "rm -f ../local_theater.db && npm run dev",
    "dev:backup": "cp ../local_theater.db ../backups/local_theater_$(date +%Y%m%d_%H%M%S).db",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all"
  },
  "keywords": [
    "theater",
    "equipment",
    "catalog",
    "management"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.3.0",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.31.1",
    "sharp": "^0.34.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.0",
    "sqlite3": "^5.1.7"
  }
}
